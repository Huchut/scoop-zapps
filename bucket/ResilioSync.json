{
    "version": "2.6.3.1340",
    "homepage": "https://www.resilio.com/individuals/",
    "url": "https://download-cdn.resilio.com/stable/windows64/Resilio-Sync_x64.exe#dl.exe",
    "installer": {
        "script": [
            "Invoke-ExternalCommand \"$dir\\dl.exe\" -ArgumentList @('/PERFORMINSTALL','/S') -RunAs | Out-Null"
        ],
        "keep": "true"
    },
    "uninstaller": {
        "script": [
            "Invoke-ExternalCommand \"$dir\\dl.exe\" -ArgumentList @('/UNINSTALL','/S','/REMSETTINGS') -RunAs | Out-Null"
        ]
    }
}